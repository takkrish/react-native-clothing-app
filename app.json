{
	"expo": {
		"scheme": "react-native-clothing-app",
		"web": {
			"bundler": "metro",
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"@react-native-google-signin/google-signin",
			"expo-router",
			[
				"@stripe/stripe-react-native",
				{
					"enableGooglePay": true
				}
			],
			[
				"expo-build-properties",
				{
					"ios": {
						"deploymentTarget": "13.0"
					}
				}
			]
		],
		"userInterfaceStyle": "automatic",
		"name": "react-native-clothing-app",
		"slug": "react-native-clothing-app",
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.takkrish.reactnativeclothingapp"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.takkrish.reactnativeclothingapp"
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "a1658db0-0ad8-49e7-bad4-b1b8f542d4a9"
			}
		},
		"owner": "takkrish",
		"orientation": "portrait",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/a1658db0-0ad8-49e7-bad4-b1b8f542d4a9"
		},
		"version": "1.0.0",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"]
	}
}
